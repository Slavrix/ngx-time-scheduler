{
  "name": "ngx-time-scheduler-extend",
  "version": "1.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "ng:build:demo": "ng build --aot --prod --base-href=https://slavrix.github.io/ngx-time-scheduler/",
    "deploy": "npm run ng:build:demo && gh-pages -d dist/demo",
    "build_date_adapters": "tsc -p tsconfig-date-adapters.json && tsc -p tsconfig-date-adapters.json --module es2015 --outDir ./dist/ngx-time-scheduler/lib/date-adapters/esm",
    "build_lib": "ng build NgxTimeScheduler",
    "copy_date_adapters": "cp-cli projects/ngx-time-scheduler/util/date-adapter-package-date-fns.json dist/ngx-time-scheduler/lib/date-adapters/date-fns/package.json & cp-cli projects/ngx-time-scheduler/util/date-adapter-package-moment.json dist/ngx-time-scheduler/lib/date-adapters/moment/package.json",
    "copy_license": "cp ./LICENSE ./dist/ngx-time-scheduler",
    "copy_readme": "cp ./README.md ./dist/ngx-time-scheduler",
    "copy_files": "npm run copy_license && npm run copy_readme && npm run copy_date_adapters",
    "npm_pack": "cd dist/ngx-time-scheduler && npm pack",
    "package": "npm run build_lib && npm run build_date_adapters && npm run copy_files && npm run npm_pack",
    "publish": "cd dist/ngx-time-scheduler && npm publish"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.0.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "calendar-utils": "^0.8.4",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.0.0",
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^14.0.13",
    "cp-cli": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "gh-pages": "^3.0.0",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.1.2",
    "protractor": "^7.0.0",
    "typescript": "4.2.4"
  }
}
